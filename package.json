{
  "name": "clickuplink",
  "displayName": "ClickUp Link",
  "description": "VSCode extension for ClickUp integration using React-inspired architecture.",
  "version": "0.0.1",
  "publisher": "your-publisher-name", 
  "author": {
    "name": "ClickUp Link Team"
  },
  "homepage": "https://github.com/your-username/clickuplink",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/clickuplink"
  },
  "extensionDependencies": [],
  "extensionPack": [],
  "icon": "resources/icon.png",
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "extensionKind": ["workspace"],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": false
    }
  },
  "browser": "./out/extension.js",  "activationEvents": [
    "onStartupFinished",
    "onUri",
    "onCommand:clickuplink.login",
    "onCommand:clickuplink.enterCode",
    "onCommand:clickuplink.logout",
    "onCommand:clickuplink.status",
    "onCommand:clickuplink.testNavigation",
    "onCommand:clickuplink.navigateProjects",
    "onCommand:clickuplink.navigateFolders",
    "onCommand:clickuplink.navigateLists",
    "onCommand:clickuplink.navigateTasks",
    "onCommand:clickuplink.navigateBack",
    "onCommand:clickuplink.testCache"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clickuplink.login",
        "title": "ClickUp: Start Authentication"
      },
      {
        "command": "clickuplink.enterCode", 
        "title": "ClickUp: Enter Authorization Code"
      },
      {
        "command": "clickuplink.logout",
        "title": "ClickUp: Reset Authentication"
      },      {
        "command": "clickuplink.status",
        "title": "ClickUp: Show Authentication Status"
      },
      {
        "command": "clickuplink.navigateProjects",
        "title": "ClickUp: Navigate Projects"
      },
      {
        "command": "clickuplink.navigateFolders",
        "title": "ClickUp: Navigate Folders"
      },
      {
        "command": "clickuplink.navigateLists",
        "title": "ClickUp: Navigate Lists"
      },
      {
        "command": "clickuplink.navigateTasks",
        "title": "ClickUp: Navigate Tasks"
      },      {
        "command": "clickuplink.navigateBack",
        "title": "ClickUp: Navigate Back"
      },
      {
        "command": "clickuplink.testNavigation",
        "title": "ClickUp: Test Navigation Flow"
      },
      {
        "command": "clickuplink.testCache",
        "title": "ClickUp: Test Cache Functionality"
      }
    ],
    "configuration": {
      "title": "ClickUp Link",
      "properties": {
        "clickupLink.workspaceId": {
          "type": "string",
          "default": null,
          "description": "ID of the active ClickUp workspace"
        },
        "clickupLink.theme": {
          "type": "string",
          "enum": ["light", "dark"],
          "default": "light",
          "description": "Theme for ClickUp Link UI elements"
        },
        "clickupLink.notificationsEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable notifications"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "typescript": "^4.8.0",
    "vscode-test": "^1.6.0"
  },
  "dependencies": {
    "axios": "^1.10.0"
  }
}
